                    android:id="@+id/solar1"
                    android:layout_width="70dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1.5dp"
                    android:layout_weight="1"
                    android:padding="1dp"
                    android:text="1"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/solar22"
                    android:layout_width="143dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1.5dp"
                    android:padding="1dp"
