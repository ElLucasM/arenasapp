                    android:layout_margin="1.5dp"
                    android:padding="1dp"
                    android:text="20"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/solar19"
                    android:layout_width="70dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1.5dp"
                    android:padding="1dp"
                    android:text="19"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/solar18"
                    android:layout_width="70dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1.5dp"
                    android:padding="1dp"
                    android:text="18"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/solar17"
                    android:layout_width="70dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1.5dp"
                    android:padding="1dp"
                    android:text="17"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/solar16"
                    android:layout_width="70dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1.5dp"
                    android:padding="1dp"
